package Figura;


public class Main {
   public static void main(String[] args) {
       Quadrado q = new Quadrado(5);
       System.out.println(q.calcularArea());


       Triangulo t = new Triangulo(6, 2 );
       System.out.println(t.calcularArea());


       CalculodeArea soma = new CalculodeArea();
       System.out.println(soma,somarAreas(a.t));
   }


}
